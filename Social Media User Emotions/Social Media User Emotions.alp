		<JavaClass>
			<Id>1729562362066</Id>
			<Name><![CDATA[SentimentAnalysis]]></Name>
			<Text><![CDATA[  /**
 * SentimentAnalysis
 */	
import edu.stanford.nlp.ling.CoreAnnotations;
import edu.stanford.nlp.ling.CoreLabel;
import edu.stanford.nlp.pipeline.Annotation;
import edu.stanford.nlp.pipeline.StanfordCoreNLP;
import edu.stanford.nlp.util.CoreMap;


import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;
import java.util.Arrays;
import java.util.HashMap;
import java.util.List;
import java.util.Properties;

public class SentimentAnalysis implements Serializable {
	
	private HashMap<String, HashMap<String, Integer>> nrcLexicon;
    /**
     * Default constructor
     */
    public SentimentAnalysis() {
    	nrcLexicon = new HashMap<>();
    	
    }
    
    public void loadNRCLexicon(String filePath) throws IOException {
        BufferedReader reader = new BufferedReader(new FileReader(filePath));
        String line;

        while ((line = reader.readLine()) != null) {
            String[] parts = line.split("\t");
            String word = parts[0];
            String emotion = parts[1];
            int association = Integer.parseInt(parts[2]);

            // Store emotion mapping
            if (!nrcLexicon.containsKey(word)) {
                nrcLexicon.put(word, new HashMap<>());
            }
            nrcLexicon.get(word).put(emotion, association);
        }

        reader.close();
    }

    // Tokenize input text
    public List<String> tokenizeText(String text) {
        text = text.toLowerCase();
        return Arrays.asList(text.split("\\s+"));  // Split by spaces
    }

    public String PresentForm(String sentence) {
        // Set up the pipeline
        Properties props = new Properties();
        props.setProperty("annotators", "tokenize,ssplit,pos,lemma");
        props.setProperty("outputFormat", "text");
        StanfordCoreNLP pipeline = new StanfordCoreNLP(props);

        // StringBuilder to construct the modified sentence
        StringBuilder modifiedSentence = new StringBuilder();

        // Create an Annotation object
        Annotation annotation = new Annotation(sentence);

        // Annotate the text
        pipeline.annotate(annotation);

        // Iterate through sentences
        List<CoreMap> sentences = annotation.get(CoreAnnotations.SentencesAnnotation.class);
        for (CoreMap sent : sentences) {
            // Iterate through tokens
            for (CoreLabel token : sent.get(CoreAnnotations.TokensAnnotation.class)) {
                String word = token.word();
                String pos = token.get(CoreAnnotations.PartOfSpeechAnnotation.class);
                String lemma = token.get(CoreAnnotations.LemmaAnnotation.class);

                if (pos.startsWith("VB")) { // Check if the token is a verb
                    String presentForm = convertToPresent(lemma);
                    System.out.println("Original verb: " + word + ", Present Form: " + presentForm);
                    modifiedSentence.append(presentForm).append(" "); // Append present form
                }
                else if (pos.startsWith("JJ")) { // Check if the token is an adjective
                    String baseForm = convertToAdjective(lemma);
                    System.out.println("Original adjective: " + word + ", Present Form: " + baseForm);
                    modifiedSentence.append(baseForm).append(" "); // Append base form of adjective
                } else if (pos.startsWith("RB")) { // Check if the token is an adverb
                    String baseForm = convertToAdverb(lemma);
                    System.out.println("Original adverb : " + word + ", Present Form: " + baseForm);
                    modifiedSentence.append(baseForm).append(" "); // Append base form of adverb
                }
                else {
                    modifiedSentence.append(word).append(" "); // Append unchanged words
                }
            }
        }
        //System.out.println("Modified sentence: " + modifiedSentence.toString().trim());
        return modifiedSentence.toString().trim();
    }

    // Simple function to convert to present tense
    private String convertToPresent(String verb) {
        // Basic rules; expand this for more complexity
        if (verb.equals("was")) return "is";
        if (verb.equals("were")) return "are";
        if (verb.endsWith("ed")) return verb.substring(0, verb.length() - 2); // rudimentary for regular verbs
        // Add more rules for irregular verbs
        return verb; // Default case
    }

    // Function to convert adjectives to their base form
    private String convertToAdjective(String adjective) {
        // Basic rules; expand this for more complexity
        if (adjective.endsWith("est")) {
            return adjective.substring(0, adjective.length() - 3); // Remove -est
        }
        if (adjective.endsWith("er")) {
            return adjective.substring(0, adjective.length() - 2); // Remove -er
        }
        // You can add more rules or a dictionary for irregular adjectives if needed
        return adjective; // Default case
    }

    // Function to convert adverbs to their base form
    private String convertToAdverb(String adverb) {
        // Basic rules; expand this for more complexity
        if (adverb.endsWith("ly")) {
            return adverb.substring(0, adverb.length() - 2); // Remove -ly
        }
        // You can add more rules or a dictionary for irregular adverbs if needed
        return adverb; // Default case
    }
   

    // Method to generate emotion vector from text
    public HashMap<String, Integer> generateEmotionVector(String text) {
        HashMap<String, Integer> emotionVector = new HashMap<>();
        // Initialize emotions (Ekman's six emotions)
        emotionVector.put("joy", 0);
        emotionVector.put("sadness", 0);
        emotionVector.put("anger", 0);
        emotionVector.put("surprise", 0);
        emotionVector.put("fear", 0);
        emotionVector.put("disgust", 0);
        //System.out.println(emotionVector);
        // Tokenize the text
        List<String> tokens = tokenizeText(text);

        // Iterate over each token and update the emotion vector
        for (String token : tokens) {
            if (nrcLexicon.containsKey(token)) {

                HashMap<String, Integer> emotions = nrcLexicon.get(token);
                //System.out.println(token);
                //System.out.println(emotions);
                for (String emotion : emotions.keySet()) {

                    //System.out.println(emotion);
                    int association = emotions.get(emotion);
                    //System.out.println(association);

                    if (emotionVector.containsKey(emotion)) {
                        if (association == 1) {

                            emotionVector.put(emotion, emotionVector.get(emotion) + 1);
                        }
                    }
                }
                //System.out.println(token);
                //System.out.println(emotionVector);
            }

        }


        // Scale the emotion scores
        int totalWords = tokens.size();
        for (String emotion : emotionVector.keySet()) {
            int count = emotionVector.get(emotion);
            double proportion = (double) count / totalWords;
            if (proportion == 0) {
                emotionVector.put(emotion, 0);
            } else if (proportion < 0.15) {
                emotionVector.put(emotion, 1);
            } else if (proportion < 0.30) {
                emotionVector.put(emotion, 2);
            } else if (proportion < 0.45) {
                emotionVector.put(emotion, 3);
            } else {
                emotionVector.put(emotion, 4);
            }
        }

        return emotionVector;
    }


	@Override
	public String toString() {
		return super.toString();
	}

	/**
	 * This number is here for model snapshot storing purpose<br>
	 * It needs to be changed when this class gets changed
	 */ 
	private static final long serialVersionUID = 1L;

}]]></Text>
