				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>RETURNS_VALUE</ReturnModificator>
					<ReturnType><![CDATA[double[]]]></ReturnType>
					<Id>1729629118192</Id>
					<Name><![CDATA[computeEmotionFromPost]]></Name>
					<Description><![CDATA[Function to calculate emotion scores based on the emotional values derived from a set of posts, considering both the maximum emotion value and the average emotion value weighted by a susceptibility factor.]]></Description>
					<X>160</X><Y>160</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[emotionVector]]></Name>
						<Type><![CDATA[HashMap<String,Integer>]]></Type>
					</Parameter>
					<Parameter>
						<Name><![CDATA[susceptability]]></Name>
						<Type><![CDATA[double]]></Type>
					</Parameter>
					<Body><![CDATA[double[] emotionScores = new double[6];  // Represents joy, sadness, anger, etc.
    
    // Compute the weighted combination of maximum and average emotions
    for (String emotion : emotionVector.keySet()) {
        int emotionValue = emotionVector.get(emotion);
        
        // Add the max-weighted contribution (β) and average-weighted contribution (1-β)
        double maxValue = Math.max(emotionValue, 0);  // Max emotion for the post
        double avgValue = (emotionValue / (double) no_Posts);  // Average emotion for the post
        
        // Combine using susceptibility
        if (emotion.equals("joy")) {
            emotionScores[0] = susceptability * maxValue + (1 - susceptability) * avgValue;
        }
        if (emotion.equals("sadness")) {
            emotionScores[1] = susceptability * maxValue + (1 - susceptability) * avgValue;
        }
        if (emotion.equals("anger")) {
            emotionScores[2] = susceptability * maxValue + (1 - susceptability) * avgValue;
        }
        if (emotion.equals("surprise")) {
            emotionScores[3] = susceptability * maxValue + (1 - susceptability) * avgValue;
        }
        if (emotion.equals("fear")) {
            emotionScores[4] = susceptability * maxValue + (1 - susceptability) * avgValue;
        }
        if (emotion.equals("disgust")) {
            emotionScores[5] = susceptability * maxValue + (1 - susceptability) * avgValue;
        }
        
        
 }
 return emotionScores;]]></Body>
